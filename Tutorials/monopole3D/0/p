FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 101325;

boundaryField
{
    free
    {
        type            waveTransmissive;
        field           p;
        phi             phi;
        rho             rho;
        psi             thermo:psi;
        gamma           1.4;
        value           uniform 101325;
    }

    sphere
    {
//        type            uniformFixedValue;
//        uniformValue    sine;
//        uniformValueCoeffs
//        {
//            amplitude   10.0;
//            scale       1.0;
//            level       101325;
//            frequency   constant 100;
//        }
        type            zeroGradient;
    }

    #include "cyc"
}

