FwhSurfacesSphere-Patch
{
        type            FfowcsWilliamsHawkings;
        formulationType Farassat1AFormulation;
        #include        "commonSettings";
        patches ();
        interpolationScheme cellPoint;

        surfaces
        (
                sphere
                {
                    type            patch;
                    patches         (sphere);
                    interpolate     false;
                }
        );
        
        cleanFreq       100;
        nonUniformSurfaceMotion true;
        Ufwh    (.0 .0 .0);
        U0      (.0 .0 .0);
        fixedResponseDelay false;
        responseDelay   0.01;
}

